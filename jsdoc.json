{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": [
      "src/"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "recurse": true,
    "template": "node_modules/better-docs",
    "readme": "README.md",
    "tutorials": "./examples"
  }
}
